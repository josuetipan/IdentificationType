generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model identificationTypes {
  id   String @id @db.Uuid @default(uuid())
  name String
}

model users {
  id        String   @id @db.Uuid
  name      String
  email     String   @unique
  createdAt DateTime @default(now())
  updatedAt DateTime

  @@index([name, email])
}

