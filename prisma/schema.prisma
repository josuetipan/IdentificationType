generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model document_type {
  name                   String
  createdAt              DateTime @default(now())
  updatedAt              DateTime
  id_type_identification String   @id @db.Uuid
}

model users {
  id        String   @id @default(uuid()) @db.Uuid
  name      String
  email     String   @unique
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
